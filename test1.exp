#!/bin/expect -f

# TEST: File not in directory. and nothing saved (file is empty).
if { [llength $argv] != 1 } {
    puts "ERROR: test1.exp called without a file. [llength $argv] given";
    puts "\tUsage: ./test1 \[file\]";
    exit 1;
}
set FILE [lindex $argv 0];

spawn dear-diary $FILE;
expect $FILE;
send "aHello World!\x1B:q!\r";
expect eof;
exit 0;
